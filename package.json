{
  "name": "node-modules-check",
  "version": "0.0.1",
  "description": "Checks packages in node_modules",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mos t",
    "md": "mos"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/node-modules-check.git"
  },
  "keywords": [
    "modules"
  ],
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "author": {
    "name": "Zoltan Kochan",
    "url": "https://www.kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/node-modules-check/issues"
  },
  "homepage": "https://github.com/pnpm/node-modules-check#readme",
  "dependencies": {
    "dependency-check": "^3.0.0",
    "find-packages": "^8.0.0",
    "thenify": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "typescript": "^3.0.0"
  }
}
